{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Tests Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "./build/${relativeFileDirname}/${fileBasenameNoExtension}.js",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			],
			"runtimeArgs": [
				"--enable-source-maps"
			]
		}
	]
}