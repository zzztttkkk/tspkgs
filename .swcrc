{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": true
		},
		"target": "esnext",
		"loose": false,
		"externalHelpers": true,
		"keepClassNames": true,
		"transform": {
			"decoratorMetadata": true
		}
	},
	"minify": false,
	"sourceMaps": true,
	"module": {
		"type": "es6",
		"strict": true
	}
}
